{
  "expo": {
    "name": "euroviaje",
    "slug": "euroviaje",
    "version": "1.0.0",
    "scheme": "euroviaje",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.guillechague.euroviaje"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "b5ee135b-a2f8-405f-8c43-6b8fd1547a6a"
      }
    }
  }
}
